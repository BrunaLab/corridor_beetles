	title = {Copro-necrophagous beetles ({Coleoptera}: {Scarabaeidae}: {Scarabaeinae}) from {Atlantic} {Forest} fragments of {Silveira} {Martins}, {Rio} {Grande} do {Sul}, {Brazil}},
	volume = {102},
