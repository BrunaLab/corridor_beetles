	keywords = {alpha diversity beta diversity dipterocarp forest ethanol trap flight intercept trap effective number of species species-diversity beta-diversity herbivorous insects host-specificity alpha-pinene rain-forest ethanol col similarity temperate},
	pages = {1461--1470},
