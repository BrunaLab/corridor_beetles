@article{kalisz_soil_1984,
	title = {Soil {Mixing} by {Scarab} {Beetles} and {Pocket} {Gophers} in {North}‚Äê{Central} {Florida}},
	volume = {48},
	issn = {0361-5995, 1435-0661},
	url = {https://acsess.onlinelibrary.wiley.com/doi/10.2136/sssaj1984.03615995004800010031x},
	doi = {10.2136/sssaj1984.03615995004800010031x},
	abstract = {Soil mixing by scarab beetles (Peltotrupes youngi) and pocket gophers (Geomys pinetus) was studied on deep sands in north-central Florida. Mound construction was monitored for 2 y on longleaf pine (Pinus palustris) (LLP) sites with dense and sparse wiregrass (Aristida stricta), and on sand pine (P. clausa) (SP) sites with dense and sparse scrub (Quercus spp.) understories. Adult beetles were active from November to April and constructed vertical tunnels at least as deep as 360 cm. Soil deposited in beetle mounds averaged 20 and 580 kg ha"1 yr{\textasciitilde}' in SP stands with dense and sparse understories, respectively, and 1510 and 3700 kg ha ' yr ' in LLP stands with dense and sparse grass. Pocket gophers were absent from SP, but mounding averaged 8160 and 290 kg ha{\textasciitilde}' yr'1 on LLP sites with dense and sparse grass. Mounding by beetles was as high as 7800 kg ha{\textasciitilde}' yr"1, and pocket gopher mounds covered 4\% of the surface area and weighed 37 500 kg ha'1 on some LLP sites. On areas with chemically contrasting soil layers, beetle mounds contained significantly greater amounts of extractable P, Ca, and Mg than the surrounding 0- to 10-cm soil. Burrowing and upward soil transport also alter surface textures and produce tunnels that may function as root pathways through the generally dense subsoil. The occurrence and thickness of A horizons under LLP and E horizons under SP are also related to differences in extent of faunal mixing between the two vegetations.},
	language = {en},
	number = {1},
	urldate = {2023-09-29},
	journal = {Soil Science Society of America Journal},
	author = {Kalisz, P. J. and Stone, E. L.},
	month = jan,
