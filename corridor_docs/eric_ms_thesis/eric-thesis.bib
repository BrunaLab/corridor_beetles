	year = {2024},
	note = {\_eprint: https://besjournals.onlinelibrary.wiley.com/doi/pdf/10.1111/1365-2435.14607},
	keywords = {Scarabaeinae, introduced species, Mesocosm experiment, nature-based solutions, nitrogen cycling, plant productivity, soil processes, temporal diversity, quant},
	pages = {1971--1983},
	file = {Full Text PDF:C\:\\Users\\erec1\\Zotero\\storage\\F28LXJLV\\Hasan et al. - 2024 - Dung beetles drive direct and indirect changes in .pdf:application/pdf;Snapshot:C\:\\Users\\erec1\\Zotero\\storage\\MJJ9WAHY\\1365-2435.html:text/html},
