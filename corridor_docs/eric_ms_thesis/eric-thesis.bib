	keywords = {diversity, species richness, Hill numbers, abundance data, incidence data, sample coverage, extrapolation, rarefaction, interpolation, prediction},
	pages = {45--67},
	annote = {‚Äúsample size (the sampling problem).‚Äù (Chao et al., 2014, p. 45)
