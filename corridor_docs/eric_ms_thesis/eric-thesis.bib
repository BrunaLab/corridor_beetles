@article{bertone_seasonal_2005,
	title = {Seasonal {Activity} and {Species} {Composition} of {Dung} {Beetles} ({Coleoptera}: {Scarabaeidae} and {Geotrupidae}) {Inhabiting} {Cattle} {Pastures} in {North} {Carolina}},
