}

@article{fiene_surveys_2011,
	title = {Surveys of {Arkansas} {Dung} {Beetles} ({Coleoptera}: {Scarabaeidae} and {Geotrupidae}): {Phenologies}, {Mass} {Occurrences}, {State} and {Distributional} {Records}},
