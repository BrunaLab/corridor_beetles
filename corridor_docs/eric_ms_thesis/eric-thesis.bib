	pages = {169--172},
	file = {Kalisz and Stone - 1984 - Soil Mixing by Scarab Beetles and Pocket Gophers i.pdf:C\:\\Users\\erec1\\Zotero\\storage\\6CUZ8UYZ\\Kalisz and Stone - 1984 - Soil Mixing by Scarab Beetles and Pocket Gophers i.pdf:application/pdf},
}

