	file = {Jost - 2006 - Entropy and diversity.pdf:C\:\\Users\\erec1\\Zotero\\storage\\2R5D9FMC\\Jost - 2006 - Entropy and diversity.pdf:application/pdf;Snapshot:C\:\\Users\\erec1\\Zotero\\storage\\9B3BSDCP\\j.2006.0030-1299.14714.html:text/html},
