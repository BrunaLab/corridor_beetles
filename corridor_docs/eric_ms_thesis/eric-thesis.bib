	keywords = {Scarabaeinae, functional groups, prop, Cattle ranching, Ecosystem functions and services, management history, Traits},
	pages = {1084009},
	file = {Arellano et al. - 2023 - Dung beetles (Coleoptera Scarabaeidae) in grazing.pdf:C\:\\Users\\erec1\\Zotero\\storage\\SXSFV8I7\\Arellano et al. - 2023 - Dung beetles (Coleoptera Scarabaeidae) in grazing.pdf:application/pdf},
