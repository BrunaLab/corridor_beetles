	keywords = {Dung beetle, Homing, Navigation, Path integration, Scarabaeus, Search},
	pages = {1161--1175},
	file = {Full Text PDF:C\:\\Users\\erec1\\Zotero\\storage\\I5TBNYMQ\\Dacke et al. - 2020 - A dung beetle that path integrates without the use.pdf:application/pdf},
}

