	keywords = {Grassland, Carbon and nitrogen cycling, Dung decomposition, Pasture, Plant growth},
	pages = {673--684},
	file = {Full Text PDF:C\:\\Users\\erec1\\Zotero\\storage\\DVV6E4F4\\Yoshitake et al. - 2014 - Deposition and decomposition of cattle dung and it.pdf:application/pdf;Snapshot:C\:\\Users\\erec1\\Zotero\\storage\\BFUMS73W\\s11284-014-1153-2.html:text/html},
