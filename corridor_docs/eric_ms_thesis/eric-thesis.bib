@article{burt_ants_2022,
	title = {Ants disperse seeds farther in habitat patches with corridors},
	volume = {13},
	copyright = {¬© 2022 The Authors. Ecosphere published by Wiley Periodicals LLC on behalf of The Ecological Society of America.},
	issn = {2150-8925},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ecs2.4324},
	doi = {10.1002/ecs2.4324},
	abstract = {Habitat fragmentation impacts ecosystems worldwide through habitat loss, reduced connectivity, and edge effects. Yet, these landscape factors are often confounded, leaving much to be investigated about their relative effects, especially on species interactions. In a landscape experiment, we investigated the consequences of connectivity and edge effects for seed dispersal by ants. We found that ants dispersed seeds farther in habitat patches connected by corridors, but only in patch centers. We did not see an effect on the total number of seeds moved or the rate ants detected seeds. Furthermore, we did not see any differences in ant community composition across patch types, suggesting that shifts in ant behavior or other factors increased ant seed dispersal in patches connected by corridors. Long-distance seed dispersal by ants that requires an accumulation of short-distance dispersal events over generations may be an underappreciated mechanism through which corridors increase plant diversity.},
	language = {en},
	number = {12},
	urldate = {2023-09-12},
	journal = {Ecosphere},
	author = {Burt, Melissa A. and Resasco, Julian and Haddad, Nick M. and Whitehead, Susan R.},
	year = {2022},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ecs2.4324},
	keywords = {fragmentation, ants, seed dispersal, corridors, edge effects, connectivity, myrmecochory, prop},
	pages = {e4324},
	annote = {‚ÄúOur results show that connecting fragmented habitat patches via corridors may have important effects on ant-mediated seed dispersal.‚Äù (Burt et al., 2022, p. 6)
},
	file = {Full Text PDF:C\:\\Users\\erec1\\Zotero\\storage\\9KZ7NFBP\\Burt et al. - 2022 - Ants disperse seeds farther in habitat patches wit.pdf:application/pdf;Snapshot:C\:\\Users\\erec1\\Zotero\\storage\\6RAV8Q8B\\ecs2.html:text/html},
}

@article{fletcher_is_2018,
