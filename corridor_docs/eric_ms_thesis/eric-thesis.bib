	keywords = {fragmentation, corridors, Black-throated Huet-huets, Chucao Tapaculo, landscape connections, patch networks, Pteroptochos tarnii, Rhinocryptidae, Scelorchilus rubecula, South American temperate rain forest, sustainable populations, umbrella species},
	pages = {2152--2163},
	file = {Full Text PDF:C\:\\Users\\erec1\\Zotero\\storage\\9EPCQ93S\\Castell√≥n and Sieving - 2007 - Patch Network Criteria for Dispersal-Limited Endem.pdf:application/pdf;Snapshot:C\:\\Users\\erec1\\Zotero\\storage\\P7PAMSZZ\\06-0945.html:text/html},
