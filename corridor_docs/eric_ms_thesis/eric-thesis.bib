	journal = {Ecology},
	author = {Resasco, Julian and Haddad, Nick M. and Orrock, John L. and Shoemaker, DeWayne and Brudvig, Lars A. and Damschen, Ellen I. and Tewksbury, Joshua J. and Levey, Douglas J.},
	year = {2014},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1890/14-0169.1},
	keywords = {biodiversity, habitat fragmentation, ants, Solenopsis invicta, species richness, connectivity, biological invasion, corridor experiment, fire ant, invasive ants, monogyne vs. polygyne ants, Savannah River Site, South Carolina, USA, prop, yep},
	pages = {2033--2039},
	file = {Full Text PDF:C\:\\Users\\erec1\\Zotero\\storage\\7CCJEMHK\\Resasco et al. - 2014 - Landscape corridors can increase invasion by an ex.pdf:application/pdf;Snapshot:C\:\\Users\\erec1\\Zotero\\storage\\Y6PWIW8Y\\14-0169.html:text/html},
