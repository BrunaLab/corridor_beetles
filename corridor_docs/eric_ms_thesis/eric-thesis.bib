	title = {Dung beetle assemblages, dung removal and secondary seed dispersal: data from a large-scale, multi-site experiment in the {Western} {Palaearctic}},
	volume = {10},
	issn = {1948-6596},
	shorttitle = {Dung beetle assemblages, dung removal and secondary seed dispersal},
	url = {https://escholarship.org/uc/item/3bw905b1},
